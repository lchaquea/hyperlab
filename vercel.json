{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/propuesta-grupo500/index.html",
      "destination": "/propuesta-grupo500",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 